<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="entity.Content" table="CONTENT">

        <id name="id" type="int">
            <generator class="native"/>
        </id>
        <property name="ip1" type="java.lang.String"/>
        <property name="ip2" type="java.lang.String"/>
        <property name="account1" type="java.lang.String"/>
        <property name="account2" type="java.lang.String"/>
        <property name="sim1" type="java.lang.String"/>
        <property name="sim2" type="java.lang.String"/>
        <property name="sendtime" type="timestamp"/>
        <property name="savepath" type="java.lang.String"/>
        <property name="fcontent" type="binary"/>
        <property name="flag" type="java.lang.Integer"/>
        <property name="text" type="java.lang.String"/>
    </class>

</hibernate-mapping>
